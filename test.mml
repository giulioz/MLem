type Boolean = true | false;
let Y = \f -> ((\x -> (x x)) (\y -> (f (\x -> ((y y) x)))));
let sub = \x -> \y -> (sum x (neg y));

let fibonacci = Y (\f -> \x -> 
  match x with
  | 0 -> 0
  | 1 -> 1
  | x -> (
    let a = (sub x) 1;
    let b = (sub x) 2;
    let fa = f a;
    let fb = f b;
    sum fa fb
  )
);

match (fibonacci 8) with
| 21 -> true
| _ -> false
